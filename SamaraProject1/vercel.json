{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "wwwroot"
      }
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/imagenes/(.*)",
      "dest": "/imagenes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}